#include<bits/stdc++.h>
using namespace std;
void changeCase(string &str)
{
    for(int i=0;i<str.length();i++)
    {
        char ch=str[i];
        if(ch>='a' && ch<='z')
        {
            str[i]=ch-'a'+'A';
        }
        else
        {
            str[i]=ch-'A'+'a';
        }
    }
}
int main()
{
    string str;
    cin>>str;
    changeCase(str);
    cout<<str<<endl;
    return 0;
}

